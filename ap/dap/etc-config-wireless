config wifi-device 'radio0'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0'
	option channel '120'
	option band '5g'
	option htmode 'VHT80'
	option cell_density '0'
	option country 'ZA'
	option txpower '20'
	option disabled '0'

config wifi-iface 'default_radio0'
	option device 'radio0'
	option mode 'ap'
	option ssid 'OpenFest'
	option encryption 'none'
	option network 'wifi_br wifi80'
	option macaddr '0e:50:00:00:00:05'
	option isolate '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'platform/ahb/18100000.wmac'
	option channel '1'
	option band '2g'
	option htmode 'HT20'
	option cell_density '0'
	option country 'ZA'
	option txpower '20'
	option disabled '0'

config wifi-iface 'default_radio1'
	option device 'radio1'
	option mode 'ap'
	option ssid 'OpenFest'
	option encryption 'none'
	option network 'wifi_br wifi80'
	option macaddr '0e:24:00:00:00:05'
	option isolate '1'

config wifi-iface 'wifinet2'
	option device 'radio0'
	option mode 'ap'
	option ssid 'Internal-VOC-A'
	option encryption 'psk2'
	option isolate '1'
	option network 'wifi30'
	option key '<some-pass>' #fix me
	option ieee80211w '1'
	option ocv '0'

config wifi-iface 'wifinet3'
	option device 'radio1'
	option mode 'ap'
	option ssid 'Internal-VOC-A'
	option encryption 'psk2'
	option isolate '1'
	option network 'wifi30'
	option key '<some-pass>' #fix me
	option ieee80211w '1'
	option ocv '0'

