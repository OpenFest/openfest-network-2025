config interface 'loopback'
	option device 'lo'
	option proto 'static'
	option ipaddr '127.0.0.1'
	option netmask '255.0.0.0'

config interface 'lan'
	option device 'wifi.20'
	option proto 'static'
	option ipaddr '10.20.0.100' #fix-me (set something free from .100+)
	option netmask '255.255.255.0'
	option gateway '10.20.0.1'
	list dns '1.1.1.1'

config switch
	option name 'switch0'
	option reset '1'
	option enable_vlan '1'

config switch_vlan
	option device 'switch0'
	option vlan '1'
	option ports '0t 2t'
	option vid '20'

config switch_vlan
	option device 'switch0'
	option vlan '3'
	option ports '0t 2t'
	option vid '95'

config device
	option type 'bridge'
	option name 'wifi'
	list ports 'eth0'
	option bridge_empty '1'
	option ipv6 '0'
	option macaddr '9c:d6:43:2d:b9:30' #fix-me

config bridge-vlan
	option device 'wifi'
	option vlan '80'
	list ports 'eth0:t'

config bridge-vlan
	option device 'wifi'
	option vlan '30'
	list ports 'eth0:t'

config bridge-vlan
	option device 'wifi'
	option vlan '95'
	list ports 'eth0:t'

config bridge-vlan
	option device 'wifi'
	option vlan '20'
	list ports 'eth0:t'

config device
	option type '8021q'
	option ifname 'wifi'
	option vid '80'
	option name 'wifi.80'

config switch_vlan
	option device 'switch0'
	option vlan '6'
	option ports '0t 2t'
	option vid '80'

config device
	option type '8021q'
	option ifname 'wifi'
	option vid '95'
	option name 'wifi.95'

config interface 'wifi80'
	option proto 'none'
	option device 'wifi.80'

config interface 'wifi95'
	option proto 'none'
	option device 'wifi.95'

config switch_vlan
	option device 'switch0'
	option vlan '7'
	option ports '0t 2t'
	option vid '30'

config device
	option type '8021q'
	option ifname 'wifi'
	option vid '30'
	option name 'wifi.30'

config interface 'wifi30'
	option proto 'none'
	option device 'wifi.30'


